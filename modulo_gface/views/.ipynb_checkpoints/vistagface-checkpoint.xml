<odoo>
  <data>
    <!-- definición de vista de lista explícita -->
    <record model="ir.ui.view" id="modulo_gface.lista_configuracion">
      <field name="name">Gface lista</field>
      <field name="model">modulo_gface.modulo_gface</field>
      <field name="arch" type="xml">
        <tree>
          <field name="requestor"/>
          <field name="transaction"/>
          <field name="contry"/>
          <field name="entity"/>
          <field name="user"/>
          <field name="userName"/>
          <field name="establishmentCode"/>
          <field name="electronicDevice"/>
          <field name="typeOfAsset"/>
          <field name="exchangeRate"/>
          <field name="regimenInformationIsr"/>
          <field name="electronicDeviceCreditNote"/>
          <field name="invoiceDescription"/>
        </tree>
      </field>
    </record>

    <!-- Acciones que abren vistas sobre modelos -->
    <record model="ir.actions.act_window" id="modulo_gface.action_window">
      <field name="name">Gface</field>
      <field name="res_model">modulo_gface.modulo_gface</field>
      <field name="view_mode">tree,form</field>
    </record>

    <!-- Top menu item -->
    <menuitem name="ModuloGface" id="modulo_gface.menu_root"/>
    <!-- menu categories -->
    <menuitem name="Menu 1" id="modulo_gface.menu_1" parent="modulo_gface.menu_root"/>
    <!-- actions -->
    <menuitem name="List" id="modulo_gface.menu_1_list" parent="modulo_gface.menu_1"
              action="modulo_gface.action_window"/>
  </data>
</odoo>